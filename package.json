  "license": "GPL-3.0",
  "scripts": {
    "build": "hardhat compile",
    "console": "hardhat console",
    "mainnet:deploy": "hardhat --network mainnet deploy",
    "mainnet:verify": "hardhat --network mainnet verify",
    "bsc:deploy": "hardhat --network bsc deploy",
    "bsc-mainnet-testing:deploy": "hardhat --network bsc-mainnet-testing deploy",
    "goerli:deploy": "hardhat --network goerli deploy",
    "polygon:deploy": "hardhat --network polygon deploy",
    "polygon-zkevm-testnet:deploy": "hardhat --network polygon-zkevm-testnet deploy",
    "fantom:deploy": "hardhat --network fantom deploy",
    "zksync-era-testnet:deploy": "hardhat --network zksync-era-testnet deploy",
    "arbitrum-one:deploy": "hardhat --network arbitrum-one deploy",
    "sepolia:deploy": "hardhat --network sepolia deploy",
    "hardhat:deploy": "hardhat --network hardhat deploy",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=1 hardhat test",
    "test:gas": "cross-env REPORT_GAS=true yarn test",
    "lint:solidity": "solhint 'contracts/**/*.sol'",
    "lint:fix": "prettier --write 'deploy/*.ts' 'test/*.ts' 'constants/*.ts' hardhat.config.ts"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.6.1",
