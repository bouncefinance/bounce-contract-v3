    "hardhat-contract-sizer": "^2.7.0",
    "hardhat-deploy": "^0.11.30",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "merkletreejs": "^0.3.9",
    "prettier": "^2.8.3",
    "prettier-plugin-solidity": "^1.1.2",
    "solhint": "^3.3.8",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
